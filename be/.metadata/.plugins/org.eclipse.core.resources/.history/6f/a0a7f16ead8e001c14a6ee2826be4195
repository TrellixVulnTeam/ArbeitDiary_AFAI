package com.arbietDiary.arbietdiary.configuration.model;

import com.arbietDiary.arbietdiary.secret.SecretData;

public class TokenUtils {
	public TokenUtils() {}
	public TokenUtils (long accessTokenValidation,long refreshTokenValidation) {
		SecretData.ACCESS_TOKEN_VALIDATION_SECOND = accessTokenValidation;
		SecretData.REFRESH_TOKEN_VALIDATION_SECCOND = refreshTokenValidation;
	}
}
